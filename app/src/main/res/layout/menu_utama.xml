		<LinearLayout
            android:layout_marginTop="18dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:visibility="gone">

                        <Button
                android:id="@+id/btnpup"
                style="@style/DashboardButton"
                android:layout_marginLeft="9dp"
                android:layout_weight="1"
                android:drawableTop="@drawable/pup_a"
                android:text="@string/pup"/>


      
            <Button
                android:id="@+id/btnpod"
                style="@style/DashboardButton"
                android:layout_weight="1"
                android:layout_marginLeft="9dp"
                android:drawableTop="@drawable/pod_a"
                android:text="@string/pod"/>

            <Button
                android:id="@+id/btndex"
                style="@style/DashboardButton"
                android:layout_weight="1"
                android:layout_marginLeft="9dp"
                android:drawableTop="@drawable/dex_a"
                android:text="@string/dex"/>
        </LinearLayout>

            <LinearLayout
                android:id="@+id/lin_lv"
                android:layout_width="fill_parent"
                android:layout_height="70dp"
                android:orientation="vertical"
                android:layout_gravity="center_horizontal"
                android:background="@color/blue_back_store"
                android:layout_marginBottom="10dp">
                <ListView
                    android:id="@+id/lv_kurir"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                </ListView>
            </LinearLayout>

			<LinearLayout
				android:layout_width="fill_parent"
				android:layout_height="wrap_content"
