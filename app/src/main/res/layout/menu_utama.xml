					<!--                 android:drawableTop="@drawable/posisi" -->
					<!--                 android:text="@string/GetPosition"/> -->
					<Button
						android:id="@+id/btnscd"
						style="@style/DashboardButton"
						android:layout_marginLeft="9dp"
						android:layout_weight="1"
						android:drawableTop="@drawable/icon_setor_atrex"
						android:text="@string/scd"
						android:visibility="gone" />

					<Button
						android:id="@+id/btnAssignment"
						style="@style/DashboardButton"
						android:layout_weight="1"
						android:drawableTop="@drawable/icon_assigment_atrex"
						android:text="@string/assignment" />
				</LinearLayout>

				<LinearLayout
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_gravity="center_horizontal"
					android:layout_marginTop="18dp"
					android:layout_marginBottom="27dp">

					<RelativeLayout
						android:id="@+id/badge_layout"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content">
						<RelativeLayout
							android:id="@+id/lin_btnAlfadigital"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content">
							<Button
								android:id="@+id/btnAlfadigital"
								style="@style/DashboardButton"
								android:layout_weight="1"
								android:drawableTop="@drawable/icon_alfadigital"
								android:text="@string/alfadigital" />
						</RelativeLayout>
						<TextView
							android:id="@+id/tv_badge_notification"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_alignRight="@id/lin_btnAlfadigital"
							android:background="@drawable/badge_circle3_item_count"
							android:text="0"
							android:textColor="#FFF"
							android:textSize="16sp"
							android:textStyle="bold"
							android:layout_gravity="center"/>

					</RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/badge_layout_disable"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <RelativeLayout
                            android:id="@+id/lin_btnAlfadigital_disable"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">
                            <Button
                                android:id="@+id/btnAlfadigital_disable"
                                style="@style/DashboardButton"
                                android:layout_weight="1"
                                android:drawableTop="@drawable/icon_alfadigital_disable"
                                android:text="@string/alfadigital"
                                android:visibility="gone"/>
                        </RelativeLayout>
                        <TextView
                            android:id="@+id/tv_badge_notification_disable"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignRight="@id/lin_btnAlfadigital_disable"
                            android:background="@drawable/badge_circle3_item_count"
                            android:text="0"
                            android:textColor="#FFF"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:layout_gravity="center"
                            android:visibility="gone"/>

                    </RelativeLayout>


					<Button
						android:id="@+id/btnlogout"
						style="@style/DashboardButton"
						android:layout_width="wrap_content"
						android:drawableTop="@drawable/icon_logout_atrex"
						android:text="@string/logout" />

				</LinearLayout>
			</LinearLayout>
         	
    <LinearLayout 
		android:layout_width="wrap_content"
      	android:layout_height="wrap_content"
      	android:orientation="horizontal"
      	android:layout_gravity="center_horizontal">
		<TextView
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:text="@string/footer_1"
        	android:textColor="@color/font"/>
	</LinearLayout>
	<LinearLayout 
		android:layout_width="wrap_content"
      	android:layout_height="wrap_content"
      	android:orientation="horizontal"
      	android:layout_gravity="center_horizontal">
		<TextView
        	android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	android:text="@string/footer_2"
        	android:textColor="@color/font"/>
	</LinearLayout>
	<LinearLayout 
		android:layout_width="wrap_content"
      	android:layout_height="wrap_content"
      	android:orientation="horizontal"
      	android:layout_gravity="center_horizontal">
    	<TextView
        	android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	android:text="@string/footer_3"
        	android:textColor="@color/font"/>
    </LinearLayout>
     <LinearLayout 
		android:layout_width="wrap_content"
      	android:layout_height="wrap_content"
      	android:orientation="horizontal"
      	android:layout_gravity="center_horizontal">
    	<TextView
    		android:id="@+id/showurl"
        	android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	android:text="url"
        	android:textColor="@color/font"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/txt_version_now"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:hint="version"
            android:textColor="@color/font_version" />
    </LinearLayout>
    <TextView
        android:id="@+id/txt_version"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:hint="version"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="135dp"
        android:visibility="gone" />
